/* componets/w-tab-control/w-tab-control.wxss */
.tab-bar{
 display: flex;
 height: 88rpx;
 background-color: white;
 align-items: center;
 text-align: center;
 
}
.tab-bar-item{
  flex: 1;
  height: 100%;
  line-height: 86rpx;
  border-bottom: 6rpx solid gray;
}
.active{
  color: var(--themeColor);
  border-bottom: 6rpx solid #eb5456;
}
